{
    "name": "Mark Groups Down",
    "description": "Export tabs and groups to markdown",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabGroups"
    ],
    "background": {
        "service_worker": "background.js"
    }
}